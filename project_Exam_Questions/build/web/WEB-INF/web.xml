    </servlet>
    <servlet>
        <servlet-name>AddYesNo</servlet-name>
        <servlet-class>Controller.AddYesNo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListDBI202</servlet-name>
        <servlet-class>Controller.DBI202.ListDBI202</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditController</servlet-name>
