        <servlet-name>TypeController</servlet-name>
        <servlet-name>AddController</servlet-name>
        <servlet-name>AddMutichiceTwo</servlet-name>
        <servlet-name>AddShortAnswer</servlet-name>
        <servlet-name>AddYesNo</servlet-name>
        <servlet-name>ListDBI202</servlet-name>
        <servlet-name>ListPRN211</servlet-name>
        <servlet-name>ListPRJ301</servlet-name>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeController</servlet-name>
        <servlet-class>Controller.TypeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddController</servlet-name>
        <servlet-class>Controller.AddController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMutichiceTwo</servlet-name>
        <servlet-class>Controller.AddMutichiceTwo</servlet-class>
