        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeController</servlet-name>
        <url-pattern>/html/type</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddController</servlet-name>
        <url-pattern>/html/add_mutichoice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMutichiceTwo</servlet-name>
        <url-pattern>/html/add_mutichoice_two</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddShortAnswer</servlet-name>
        <url-pattern>/html/short_answer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddYesNo</servlet-name>
        <url-pattern>/html/yes_no_answer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListDBI202</servlet-name>
        <url-pattern>/html/list_DBI202</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditController</servlet-name>
